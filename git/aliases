alias g=git
alias ga='git add'
alias gap='git add -p'
alias gb='git branch'
alias gba='git branch -a'
alias gbr='git branch -va'
alias gbrl='git branch -v'
alias gbrs='gbr | sed -e "s/^\*/ /" | sort -k 2'
alias gc='git commit -v'
alias 'gc!'='git commit -v --amend'
alias gca='git commit -v -a'
alias 'gca!'='git commit -v -a --amend'
alias gcl='git config --list'
alias gclean='git reset --hard && git clean -dfx'
alias gcm='git checkout master'
alias gco='git checkout'
alias gco33='git checkout version-3.3'
alias gcount='git shortlog -sn'
alias gcp='git cherry-pick'
alias gd='git diff'
alias gdf='git diff'
alias gdfc='git diff --cached'
alias gf='git ls-files | grep'
alias gfe='git fetch --all --prune --tags'
alias gg='git gui citool'
alias gga='git gui citool --amend'
alias ggpnp='git pull origin $(current_branch) && git push origin $(current_branch)'
alias ggpull='git pull origin $(current_branch)'
alias ggpur='git pull --rebase origin $(current_branch)'
alias ggpush='git push origin $(current_branch)'
alias git-svn-dcommit-push='git svn dcommit && git push github master:svntrunk'
alias gk='gitk --all --branches'
alias gl='git pull'
alias glg='git log --stat --max-count=5'
alias glgg='git log --graph --max-count=5'
alias glgga='git log --graph --decorate --all'
alias glo='git log --oneline'
alias glp=_git_log_prettily
alias gm='git merge'
alias gmrg='git merge --no-ff'
alias gmt='git mergetool --no-prompt'
alias gp='git push'
alias gpoat='git push origin --all && git push origin --tags'
alias gr='git remote'
alias grba='git rebase --abort'
alias grbc='git rebase --continue'
alias grbi='git rebase -i'
alias grh='git reset HEAD'
alias grhh='git reset HEAD --hard'
alias grmv='git remote rename'
alias grrm='git remote remove'
alias grset='git remote set-url'
alias grt='cd $(git rev-parse --show-toplevel || echo ".")'
alias grup='git remote update'
alias grv='git remote -v'
alias gsd='git svn dcommit'
alias gsr='git svn rebase'
alias gss='git status -s'
alias gst='git status'
alias gstuno='git status -uno'
alias gsts='git stash show --text'
alias gsu='git submodule update'
alias gup='git pull --rebase'
alias gwc='git whatchanged -p --abbrev-commit --pretty=medium'
alias gu='git fetch --all;git pull --rebase; git submodule sync ; git submodule update ; git status'
alias gur='git fetch --all;git pull --rebase; git submodule sync ; git submodule update --remote ; git status'
alias gcheck='gdfc | grep -e "[^ ] $" -e "print" -e "console"'
alias glgg='git log --stat @{upstream}..'
alias gllg='git log --stat ..@{upstream}'
alias gdgg='git diff @{upstream}..'
alias gddg='git diff ..@{upstream}'
